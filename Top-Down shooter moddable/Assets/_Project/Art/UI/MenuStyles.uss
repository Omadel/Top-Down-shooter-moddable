.menu-button {
    width: 250px;
    height: 80px;
    font-size: 49px;
}

.title {
    -unity-text-align: middle-center;
    font-size: 250px;
    text-shadow: 15px 15px 0 rgb(0, 0, 0);
    letter-spacing: 0;
    -unity-font-style: normal;
    color: rgb(188, 188, 188);
}

.unity-progress-bar__progress {
    background-color: rgba(56, 56, 56, 255);
}

.unity-progress-bar__container {
    max-height: 100%;
    min-height: 100%;
}
